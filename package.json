{
  "name": "pegavaretas",
  "displayName": "Pega Varetas",
  "version": "1.4.5",
  "description": "Um jeito divertido de contar seus pontos no pega varetas",
  "main": "index.js",
  "scripts": {
    "start": "mobileui preview",
    "res": "cordova-res-generator -p android -i ./res/icon.png -s ./res/splash.png -o ./res",
    "test": "standard"
  },
  "author": "Fernando Migliorini Ten√≥rio",
  "license": "MIT",
  "dependencies": {
    "cordova-android": "^7.1.0",
    "cordova-browser": "^5.0.3",
    "cordova-plugin-splashscreen": "^5.0.2",
    "cordova-plugin-statusbar": "^2.4.2",
    "cordova-plugin-whitelist": "^1.3.3"
  },
  "cordova": {
    "plugins": {
      "cordova-plugin-whitelist": {},
      "cordova-plugin-statusbar": {},
      "cordova-plugin-splashscreen": {}
    },
    "platforms": [
      "android",
      "browser"
    ]
  },
  "standard": {
    "globals": [
      "MobileUI",
      "alert",
      "closeAlert",
      "closeMenu",
      "cordova",
      "StatusBar",
      "backPage"
    ],
    "ignore": [
      "www/mobileui"
    ],
    "fix": true,
    "cwd": "www/*.js"
  },
  "devDependencies": {
    "cordova-res-generator": "^0.4.2",
    "standard": "^11.0.1"
  }
}